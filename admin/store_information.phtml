<%
	sql = "SELECT * FROM " & g_storeid & "_profile"
	set rs = dbconn.Execute(sql)
	if not rs.EOF then
	
		company = rs("Company")
		address = rs("Address")
		city = rs("City")
		state = rs("State")
		zip = rs("Zip")
		szCountry = rs("Country")
		phone = rs("Phone")
		email = rs("Email")
		
		profile_visa = rs("visa")
		profile_mastercard = rs("MasterCard")
		profile_americanexpress = rs("AmericanExpress")
		profile_discover = rs("Discover")
		profile_cod = rs("cod")
		profile_billme = rs("billme")
		'profile_check = rs("HasCheckPayment")
		
		
		SH_display_flag = rs("SH_display_flag")
		'Warehouse Information...
		warehouse_city = Trim(rs("warehouse_city"))
		warehouse_state = Trim(rs("warehouse_state"))
		warehouse_zip = Trim(rs("warehouse_zip"))
		warehouse_country = Trim(rs("warehouse_country"))
		assume_res = Trim(rs("assume_res"))
		
		'UPS
		sh_handling = rs("sh_handling")
		sh_handling = CStr(FormatNumber(sh_handling,2))
		sh_markup = rs("sh_markup")
		sh_markup = CStr(FormatNumber(sh_markup,2))
		ups_xml_userid = Trim(rs("ups_xml_userid"))
		ups_xml_password = Trim(rs("ups_xml_password"))
		ups_xml_key = Trim(rs("ups_xml_key"))
		ups_xml_flag = Trim(rs("ups_xml_flag"))
		sh_ups_flags = rs("sh_ups_flags")
		
		'UPS tables...(2)
		GND="0"
		DM1="0"
		DA1="0"
		DP1="0"
		DM2="0"
		DA2="0"
		DS3="0"
			
		if mid(sh_ups_flags,1,1) = "1" then
			GND = "1"
		end if
		if mid(sh_ups_flags,2,1) = "1" then
			DM1 = "1"
		end if
		if mid(sh_ups_flags,3,1) = "1" then
			DA1 = "1"
		end if
		if mid(sh_ups_flags,4,1) = "1" then
			DP1 = "1"
		end if
		if mid(sh_ups_flags,5,1) = "1" then
			DM2 = "1"
		end if
		if mid(sh_ups_flags,6,1) = "1" then
			DA2 = "1"
		end if
		if mid(sh_ups_flags,7,1) = "1" then
			DS3 = "1"
		end if
	
		'FedEx Settings
		fedex_markup = rs("fedex_markup")
		fedex_markup = CStr(FormatNumber(fedex_markup,2))
		fedex_handling = rs("fedex_handling")
		fedex_handling = CStr(FormatNumber(fedex_handling,2))
		show_fedex_tracking = rs("show_fedex_tracking")
		fedex_service_flag = Trim(rs("fedex_service_flag"))
		
		fedex_account = Trim(rs("fedex_account"))
		fedex_meter = Trim(rs("fedex_meter")) 
		
		'USPS Settings
		usps_userid = Trim(rs("usps_userid"))
		usps_password = Trim(rs("usps_password"))
		usps_markup = rs("usps_markup")
		usps_markup = CStr(FormatNumber(usps_markup,2))
		usps_handling = rs("usps_handling")
		usps_handling = CStr(FormatNumber(usps_handling,2))
		show_usps_tracking = rs("show_usps_tracking")
		usps_service_flag = Trim(rs("usps_service_flag"))
		ups_ins = rs("UPS_insurance")
		ups_ins = FormatNumber(ups_ins,2)
		
		'Table
		sh_basis = Trim(rs("sh_basis"))
		sh_custom_table_name = Trim(rs("sh_custom_table_name"))
		
	end if
	rs.Close
	set rs = nothing
	
	
	'-------------------------------------------------------------------------------------------------
	'Custom shipping table(1)
	'-------------------------------------------------------------------------------------------------
	
	dim table_data(6)
	table_data(1) = ""
	table_data(2) = ""
	table_data(3) = ""
	table_data(4) = ""
	table_data(5) = ""
	table_data(6) = ""
	
	dim store_shipping_table_rate_country(6)
	store_shipping_table_rate_country(1) = GetSettingValue("store_shipping_table_rate_country[1]", "")
	store_shipping_table_rate_country(2) = GetSettingValue("store_shipping_table_rate_country[2]", "")
	store_shipping_table_rate_country(3) = GetSettingValue("store_shipping_table_rate_country[3]", "")
	store_shipping_table_rate_country(4) = GetSettingValue("store_shipping_table_rate_country[4]", "")
	store_shipping_table_rate_country(5) = GetSettingValue("store_shipping_table_rate_country[5]", "")
	store_shipping_table_rate_country(6) = GetSettingValue("store_shipping_table_rate_country[6]", "")
	
	for kk = 1 to 6
		s_data = ""
		sql = "SELECT * FROM " & g_storeid & "_custom_shipping WHERE country_code='" & store_shipping_table_rate_country(kk) & "' ORDER BY range_from"
		
		set rsTabs = dbconn.Execute(sql)
		attr_id = 1
		do while not rsTabs.EOF
		
			tab_from = Trim(rsTabs("range_from"))
			tab_price = FormatNumber(rsTabs("range_charge"),2,,,0)
			
			s_data = s_data & "{ ""ID"": " & attr_id & ", ""From"": """ & tab_from & """, ""Price"": """ & tab_price & """},"
			
			attr_id = attr_id + 1
			rsTabs.MoveNext
		loop
		rsTabs.Close
		set rsTabs = nothing
		if Len(s_data) > 0 then
			s_data = Left(s_data, Len(s_data) - 1)
		end if
		s_data = "[" & s_data & "]"
		table_data(kk) = s_data
	next
	
	sql = "SELECT * FROM " & g_storeid & "_custom_shipping ORDER BY range_from"
	Set rsCustom = dbconn.Execute(sql)
	i = 0
	
	custom_sh_range = ""
	custom_sh_charge = ""
	
	do while not rsCustom.EOF
	  custom_sh_range = custom_sh_range & rsCustom("range_from") & "*"
	  custom_sh_charge = custom_sh_charge & FormatNumber(rsCustom("range_charge"),2) & "*"
	  rsCustom.MoveNext	
	loop
	
	if Len(custom_sh_range) > 0 then
	  custom_sh_range = Left(custom_sh_range, Len(custom_sh_range)-1)
	  custom_sh_charge = Left(custom_sh_charge, Len(custom_sh_charge)-1)
	else
	  custom_sh_range = "0"
	  custom_sh_charge = "0.00"
	end if

	rsCustom.Close
	
	
	
	'-------------------------------------------------------------------------------------------------
	
	goodgree_payment_enable = GetSettingValue("goodgree_payment_enable", "0")
	goodgree_payment_mode = GetSettingValue("goodgree_payment_mode", "sandbox")
	goodgree_payment_sandbox_sk = GetSettingValue("goodgree_payment_sandbox_sk", "")
	goodgree_payment_live_sk = GetSettingValue("goodgree_payment_live_sk", "")
	goodgree_payment_payment_action = GetSettingValue("goodgree_payment_payment_action", "authorize_capture")
	goodgree_payment_descriptor = GetSettingValue("goodgree_payment_descriptor", "")
	goodgree_payment_account_id = GetSettingValue("goodgree_payment_account_id", "")
	goodgree_payment_access_token = GetSettingValue("goodgree_payment_access_token", "")
	goodgree_payment_access_token_sandbox = GetSettingValue("goodgree_payment_access_token_sandbox", "")
	
	store_payment_gateway_enabled = GetSettingValue("store_payment_gateway_enabled", "0")
	store_payment_cc_type = GetSettingValue("store_payment_cc_type", "")
	store_payment_cc_use_card_id = GetSettingValue("store_payment_cc_use_card_id", "0")
	
	store_payment_authorizenet_api_login_id = GetSettingValue("store_payment_authorizenet_api_login_id", "")
	store_payment_authorizenet_payment_action = GetSettingValue("store_payment_authorizenet_payment_action", "authorize")
	store_payment_authorizenet_transaction_key = GetSettingValue("store_payment_authorizenet_transaction_key", "")
	
	store_payment_paypal_payflowpro_payment_action = GetSettingValue("store_payment_paypal_payflowpro_payment_action", "Authorization")
	store_payment_paypal_payflowpro_partner = GetSettingValue("store_payment_paypal_payflowpro_partner", "")
	store_payment_paypal_payflowpro_user = GetSettingValue("store_payment_paypal_payflowpro_user", "")
	store_payment_paypal_payflowpro_vendor = GetSettingValue("store_payment_paypal_payflowpro_vendor", "")
	store_payment_paypal_payflowpro_password = GetSettingValue("store_payment_paypal_payflowpro_password", "")
	
	store_payment_paypal_payflowlink_partner = GetSettingValue("store_payment_paypal_payflowlink_partner", "")
	store_payment_paypal_payflowlink_user = GetSettingValue("store_payment_paypal_payflowlink_user", "")
	store_payment_paypal_payflowlink_vendor = GetSettingValue("store_payment_paypal_payflowlink_vendor", "")
	store_payment_paypal_payflowlink_password = GetSettingValue("store_payment_paypal_payflowlink_password", "")
	
	store_payment_paypal_paymentpro_payment_action = GetSettingValue("store_payment_paypal_paymentpro_payment_action", "Authorization")
	store_payment_paypal_paymentpro_paypal_email_address = GetSettingValue("store_payment_paypal_paymentpro_paypal_email_address", "")
	store_payment_paypal_paymentpro_api_username = GetSettingValue("store_payment_paypal_paymentpro_api_username", "")
	store_payment_paypal_paymentpro_api_password = GetSettingValue("store_payment_paypal_paymentpro_api_password", "")
	store_payment_paypal_paymentpro_api_signature = GetSettingValue("store_payment_paypal_paymentpro_api_signature", "")
	store_payment_paypal_paymentpro_sandbox_mode = GetSettingValue("store_payment_paypal_paymentpro_sandbox_mode", "0")
	
	store_payment_paypal_expresscheckout_payment_action = GetSettingValue("store_payment_paypal_expresscheckout_payment_action", "0")
	store_payment_paypal_expresscheckout_paypal_email_address = GetSettingValue("store_payment_paypal_expresscheckout_paypal_email_address", "")
	store_payment_paypal_expresscheckout_api_username = GetSettingValue("store_payment_paypal_expresscheckout_api_username", "")
	store_payment_paypal_expresscheckout_api_password = GetSettingValue("store_payment_paypal_expresscheckout_api_password", "")
	store_payment_paypal_expresscheckout_api_signature = GetSettingValue("store_payment_paypal_expresscheckout_api_signature", "")
	store_payment_paypal_expresscheckout_sandbox_mode = GetSettingValue("store_payment_paypal_expresscheckout_sandbox_mode", "0")
	
	store_payment_check_enabled = GetSettingValue("store_payment_check_enabled", "0")
	store_payment_check_title = GetSettingValue("store_payment_check_title", "Check/Money Order")
	
	store_payment_purchase_order_enabled = GetSettingValue("store_payment_purchase_order_enabled", "0")
	store_payment_purchase_order_title = GetSettingValue("store_payment_purchase_order_title", "Purchase Order")
	


%>

<% for kk = 1 to 6 %>
<script>
jQuery(document).ready(function () {
		
	dataX<%=kk%> = <%=table_data(kk)%>;
	dialogX<%=kk%> = $("#dialog-table-<%=kk%>").dialog({
		title: "Add/Edit",
		autoOpen: false,
		resizable: false,
		width: '400px',
		modal: true,
		buttons: {
			"Save": SaveX<%=kk%>,
			"Cancel": function () { $(this).dialog("close"); }
		}
	});
	
	function EditX<%=kk%>(e) {
		$("#TableSHID<%=kk%>").val(e.data.id);
		$("#TableFrom<%=kk%>").val(e.data.record.From);
		$("#TableSHPrice<%=kk%>").val(e.data.record.Price);
		$("#dialog-table-<%=kk%>").dialog("open");
	}
	function DeleteX<%=kk%>(e) {
		if (confirm("Are you sure?")) {
			gridx<%=kk%>.removeRow(e.data.id);
		}
	}
	function SaveX<%=kk%>() {
		if ($("#TableSHID<%=kk%>").val()) {
			var id = parseInt($("#TableSHID<%=kk%>").val());
			gridx<%=kk%>.updateRow(id, { "ID": id, "From": $("#TableFrom<%=kk%>").val(), "Price": $("#TableSHPrice<%=kk%>").val() });
		} else {
			gridx<%=kk%>.addRow({ "ID": gridx<%=kk%>.count() + 1, "From": $("#TableFrom<%=kk%>").val(), "Price": $("#TableSHPrice<%=kk%>").val() });
		}
		$(this).dialog("close");
	}
	 
	gridx<%=kk%> = $("#gridx<%=kk%>").grid({
		dataSource: dataX<%=kk%>,
		columns: [
			{ field: "From" },
			{ field: "Price"},
			{ title: "", width: 20, type: "icon", icon: "ui-icon-pencil", tooltip: "Edit", events: { "click": EditX<%=kk%> } },
			{ title: "", width: 20, type: "icon", icon: "ui-icon-close", tooltip: "Delete", events: { "click": DeleteX<%=kk%> } }
		]
	});
	
	$("#btnAddX<%=kk%>").on("click", function () {
		$("#TableSHID<%=kk%>").val("");
		$("#TableFrom<%=kk%>").val("0");
		$("#TableSHPrice<%=kk%>").val("0.00");
		$("#dialog-table-<%=kk%>").dialog("open");
	});
	
	jQuery.ajax({
		type:"post",
		url: "api/sp_get_country_list.phtml",
		data: {'country_id':'<%=store_shipping_table_rate_country(kk)%>','isAjax':true},
		dataType: 'json',
		success: function(data) {
			var listItems= "<option value=''>-----All-----</option>";
			for (var i = 0; i < data.Country.length; i++){
				if (data.Country[i].id == '<%=store_shipping_table_rate_country(kk)%>')
				{
					listItems+= "<option value='" + data.Country[i].id + "' selected>" + data.Country[i].name + "</option>";
				}
				else
				{
					listItems+= "<option value='" + data.Country[i].id + "'>" + data.Country[i].name + "</option>";
				}
			}
			var select = jQuery("#TableSHCountry<%=kk%>"), options = '';
			select.empty(); 
			select.append(listItems);
		}
	});
	
});
</script>
